[alias]
	pr = "!f() { \
        git fetch $1 pull/$2/head:PR$2; \
      }; f"
	ra = "!f() { \
        git remote add $1 https://bitbucket.org/$2.git; \
      }; f"
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
